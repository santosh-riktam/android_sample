<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <fragment
        android:name="com.riktamtech.android.sellabike.ui.TabsFragment"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:tag="@string/frg_tab_tag" />

    <FrameLayout
        android:id="@+id/adsContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:id="@id/tab_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
        </LinearLayout>

        <com.mobfox.sdk.MobFoxView
            android:id="@+id/mobFoxView"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            publisherId="bd54f18e4782faa362bab1127a9ffdf9" android:layout_gravity="bottom"/>
        
        
    </FrameLayout>

</LinearLayout>